#include <stdio.h>
#include <stdlib.h>

int main()
{
	int  s,i,j, a, b,k, ost;
	k = 0;
	for (s = 0; s <= 999999; s++)
	{

		i = s;
		a = 0;
		b = 0;
		for (j = 1; j <= 3; j++)
		{
			ost = i % 10;
			i = i / 10;
			a = a + ost;
		};
		for ( j = 1; j <= 3; j++)
		{
			ost = i % 10;
			i = i / 10;
			b = b + ost;

		};

		if (a == b) {
			k++;
		}
	}
	printf("%d\n", k);
	system("pause");
	return 0;
}
